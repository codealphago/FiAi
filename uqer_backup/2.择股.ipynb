     "input": "import numpy as np\nimport pandas as pd\nimport datetime as dt\nimport lib.comm as comm\nimport lib.beta as beta\nimport lib.alpha as alpha\n\ntoday = comm.getPrevBusinessDay(dt.date.today(),1)\nstart = '2016-01-01'                        # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = today.strftime('%Y-%m-%d')                 #\u6700\u8fd1\u4ea4\u6613\u65e5\n#end = '2015-06-01'                          #\u56de\u6d4b\u7ed3\u675f\u65f6\u95f4  \n\nbenchmark = 'HS300'                        # \u7b56\u7565\u53c2\u8003\u6807\u51c6\nuniverse = set_universe('HS300') #+ set_universe('ZZ500')          # \u8bc1\u5238\u6c60\uff0c\u652f\u6301\u80a1\u7968\u548c\u57fa\u91d1\ncapital_base = 1000000                      # \u8d77\u59cb\u8d44\u91d1100W\nfreq = 'd'                                 # \u7b56\u7565\u7c7b\u578b\uff0c'd'\u8868\u793a\u65e5\u95f4\u7b56\u7565\u4f7f\u7528\u65e5\u7ebf\u56de\u6d4b\uff0c'm'\u8868\u793a\u65e5\u5185\u7b56\u7565\u4f7f\u7528\u5206\u949f\u7ebf\u56de\u6d4b\nrefresh_rate = 5                  # \u8c03\u4ed3\u9891\u7387\uff0c\u8868\u793a\u6267\u884chandle_data\u7684\u65f6\u95f4\u95f4\u9694\uff0c\u82e5freq = 'd'\u65f6\u95f4\u95f4\u9694\u7684\u5355\u4f4d\u4e3a\u4ea4\u6613\u65e5\uff0c\u82e5freq = 'm'\u65f6\u95f4\u95f4\u9694\u4e3a\u5206\u949f \n\nmax_percent_of_a_stock = 0.3 # \u5355\u652f\u80a1\u7684\u6700\u5927\u4ed3\u4f4d\nmax_stock = 5 #\u6700\u5927\u80a1\u7968\u6570\n# \u5168\u5c40\u53d8\u91cf\n\ndef initialize(account):                   # \u521d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    pass\ndef handle_data(account):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    \n    buys = []\n    sells = []\n    seclist = []\n    cur_date = account.current_date.strftime('%Y%m%d')\n    pre_date = account.previous_date\n    \n# ------------------------\u62e9\u80a1\u4ee3\u7801---------------------------------------------------------------------------------------\n\n# #\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a    \u7ecf\u5178\u4ef7\u503c\u6295\u8d44\u7b56\u7565\u7684\u4ef7\u503c\u4e94\u6cd5   \uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\n#     buylist1 = alpha.jiazhi_one(account.universe,account.current_date)\n#     buylist2 = alpha.jiazhi_two(buylist1,account.current_date)\n# #     print account.current_date,' ',len(buylist2)       \n#     buylist3 = alpha.jiazhi_three(buylist2,account.current_date)\n# #     print account.current_date,' ',len(buylist3)        \n# #\u5168\uff21\u80a1\u4e2d\uff14\uff10\u53ea\u5de6\u53f3\u80a1\u7968\u6ee1\u8db3\u6b64\u6761\u4ef6\n#     buylist4 = alpha.jiazhi_four(buylist3,account.current_date)       \n# #     print account.current_date,'buylist4 : ',len(buylist4)      \n# #\u5168\uff21\u80a1\u4e2d\uff11\uff10\uff10\u53ea--\uff13\uff10\uff10\u53ea\u5de6\u53f3\u80a1\u7968\u6ee1\u8db3\u6b64\u6761\u4ef6\n#     #buylist5 = alpha.jiazhi_five(buylist4,account.current_date)\n# #     print account.current_date,' ',len(buylist5)      \n# #\u6b64\u6761\u5728\uff21\u80a1\u5e02\u573a\u5e76\u4e0d\u9002\u7528,\u4ece\u51e0\u5e74\u7684\u56de\u6d4b\u7ed3\u679c\u6765\u770b\u53ea\u6709\u3000 (600325.XSHG \u534e\u53d1\u80a1\u4efd) \u6ee1\u8db3\u8fc7\u4e00\u6bb5\u65f6\u95f4\n# #--------------------------------------------------------------------------------------------------------------- \n# #\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a    \u7ecf\u5178\u4ef7\u503c\u6295\u8d44\u7b56\u7565\u7684\u5b89\u5168\u4e94\u6cd5   \uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\uff0a\n#     buylist6 = alpha.jiazhi_six(account.universe,account.current_date)\n# #     print account.current_date,'buylist6 : ',len(buylist6)      # 2200+ , \n# #\uff21\u80a1\u5e02\u573a\u7edd\u5927\u591a\u6570\u80a1\u7968\u6ee1\u8db3\u6b64\u6761,\u4f46\u7ecf\u8fc7\u4ef7\u503c\u4e94\u6cd5\u7b5b\u9009\u7684\u80a1\u7968\u5269\u4e0b\uff12\uff10\uff0d\uff13\uff15\u53ea\u5de6\u53f3\n#     buylist7 = alpha.jiazhi_seven(buylist6,account.current_date)        \n# #     print account.current_date, 'buylist7 : ',len(buylist7)    \n# #\u3000\uff21\u80a1\u5e02\u573a900\u5de6\u53f3\u4e2a\u80a1\u6ee1\u8db3 , \u4f46\u6ee1\u8db3__\u4ef7\u503c\u4e94\u6cd5__\u7684\u80a1\u7968\u5728\u6b64\u8f6e\u7b5b\u9009\u540e\u53ea\u5269\u4e0b\uff11\uff0c\uff12\u53ea\uff0c\u5728\u4e4b\u540e\u7684\u9009\u80a1\u4e2d\u6682\u5c4f\u853d\u6389\u6b64\u6761\u4ef6\n#     buylist8 = alpha.jiazhi_eight(buylist7,account.current_date)    \n# #     print account.current_date,'buylist8 : ',len(buylist8)      \n# #\u3000\uff21\u80a1\u5e02\u573a\u6ee1\u8db3\u6761\u4ef6\u4e2a\u80a1900--1100\u3000\uff0c\u4f46\u6ee1\u8db3__\u4ef7\u503c\u4e94\u6cd5__\u7684\u80a1\u7968\u5728\u6b64\u8f6e\u7b5b\u9009\u540e\u5269\u4e0b \uff10\uff0d\uff14\u53ea\uff0c\u5728\u4e4b\u540e\u7684\u9009\u80a1\u4e2d\u6682\u5c4f\u853d\u6389\u6b64\u6761\u4ef6\n#     buylist9 = alpha.jiazhi_nine_ten(buylist8,account.current_date)    \n# #     print account.current_date,'buylist9\u3000:  ',len(buylist9),'buylist10 : ',len(buylist10)   \n# #15\u5e74\u524d\u6ee1\u8db3__\u4ef7\u503c\u4e94\u6cd5__\u7684\u80a1\u7968\u5728\u6b64\u8f6e\u7b5b\u9009\u540e\u5269\u4e0b\uff11\uff15\u53ea\u5de6\u53f3\u3000\u6ee1\u8db3\u6761\u4ef6\uff19\u7684\u51e0\u4e4e\u90fd\u6ee1\u8db3\u6761\u4ef6\uff11\uff10\n \n\n    #\u4ed3\u4f4d\u8c03\u6574----------------------------------------\n    pos = account.avail_secpos.keys() # \u53ef\u5356\u8bc1\u5238\u5934\u5bf8\n  \n    sells = beta.sellStrategy(pos,buys)  \n    for sell_stk in sells:\n        #print(cur_date+u\"|Sell|\"+sell_stk)\n        order_to(sell_stk, 0)\n        \n    buys = beta.buyStrategy(pos,buys) \n    if len(buys) > 0 :\n        value = min(account.referencePortfolioValue / len(buys) , account.referencePortfolioValue * max_percent_of_a_stock)\n        #print value\n        for buy_stk in buys:\n            amount = 100 * int(value / account.referencePrice[buy_stk]/100)\n            if amount > 100:\n                #print cur_date+'|buy|'+buy_stk+':' ,amount  \n                order(buy_stk, amount)\n\n        \n    ",
