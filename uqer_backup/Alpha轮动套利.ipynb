     "id": "75B6A1E6EAA840409E2C5487E1F4805D",
     "input": "from CAL.PyCAL import *\nimport numpy as np\nimport pandas as pd\n\nfrom matplotlib import pylab\nimport seaborn as sns\nimport quartz\nfrom CAL.PyCAL import *\n\n#universe = set_universe(IndSW.TongXinL1, date='20141101')\nuniverse = set_universe('HS300', date='20141101')\n\nstart = '2012-01-01'                       # \u56de\u6d4b\u8d77\u59cb\u65f6\u95f4\nend = '2016-08-15'\nbenchmark = '000001.ZICN'                       # \u7b56\u7565\u53c2\u8003\u6807\u51c6\ncapital_base = 10000000                     # \u8d77\u59cb\u8d44\u91d1\nfreq = 'd' \n\ndata=DataAPI.TradeCalGet(exchangeCD=u\"XSHG\",beginDate=u\"20120101\",field=['calendarDate','isWeekEnd','isMonthEnd'],pandas=\"1\")\ndata = data[data['isMonthEnd'] == 1]\ndate_list = map(lambda x: x[0:4]+x[5:7]+x[8:10], data['calendarDate'].values.tolist())\n\n\nrefresh_rate = 1                         # \u8c03\u4ed3\u9891\u7387\ncommission = Commission(buycost=0.0008, sellcost=0.0018)\n\ncal = Calendar('China.SSE')\nperiod = Period('-1B')\n\ndef stoploss(hist,bench):\n    security_returns = (hist[len(hist)-1]-hist[0])/hist[0]\n    if security_returns<bench:\n        return True\n    else:\n        return False\n    \ndef initialize(account):                   # \u521d\u59cb\u5316\u865a\u62df\u8d26\u6237\u72b6\u6001\n    pass\n\n\ndef handle_data(account):                  # \u6bcf\u4e2a\u4ea4\u6613\u65e5\u7684\u4e70\u5165\u5356\u51fa\u6307\u4ee4\n    today = account.current_date\n    today = Date.fromDateTime(account.current_date)  # \u5411\u524d\u79fb\u52a8\u4e00\u4e2a\u5de5\u4f5c\u65e5\n    yesterday = cal.advanceDate(today, period)\n    yesterday = yesterday.toDateTime().strftime('%Y%m%d')      \n    Factor = DataAPI.MktStockFactorsOneDayGet(tradeDate=yesterday,secID=account.universe,field=['secID','PCF','PE','PB'],pandas=\"1\")\n    Factor['ticker'] = Factor['secID'].apply(lambda x: x[0:6])\n    \n    Factor.set_index('ticker',inplace=True)\n    Factorb=Factor.copy()\n    Factor['PE'] = 1.0 / Factor['PE']  # \u4f4e\u5e02\u76c8\u7387\n    factor = Factor['PE'].dropna().to_dict()\n    signal_PE = standardize(neutralize(winsorize(factor),yesterday))\n    factor = Factor['PCF'].dropna().to_dict()\n    signal_PCF = standardize(neutralize(winsorize(factor),yesterday))\n    Factor['PB'] = 1.0 / Factor['PB']  # \u4f4e\u5e02\u76c8\u7387\n    factor = Factor['PB'].dropna().to_dict()\n    signal_PB = standardize(neutralize(winsorize(factor),yesterday))\n    factor_name = ['PE','PCF','PB']\n    Total_Score = pd.DataFrame(index=Factor.index, columns=factor_name, data=0)\n    Total_Score['PE'][signal_PE.keys()] = signal_PE.values()\n    Total_Score['PCF'][signal_PCF.keys()] = signal_PCF.values()\n    Total_Score['PB'][signal_PCF.keys()] = signal_PB.values()\n    Total_Score['total_score'] = np.dot(Total_Score, np.array([0.3,0.3,0.3]))\n    total_score = Total_Score['total_score'].to_dict()\n    \n    wts = long_only(total_score,0,0.15,3,yesterday)\n    Factor['wts'] = np.nan\n    Factor['wts'][wts.keys()] = wts.values()\n    Factor=Factor.dropna(axis=0, how='any', thresh=None, subset=None, inplace=False)\n    Factor.set_index('secID', inplace=True)\n    \n    shaixuan = long_only(total_score,0,0.49,3,yesterday)\n    Factorb['shaixuan'] = np.nan\n\n    alist=[999]\n    Factorb['shaixuan'][shaixuan.keys()] = shaixuan.values()\n    Factorb=Factorb.where(Factorb.notnull(), 999)\n    Factorb=Factorb[Factorb['shaixuan'].isin(alist)]\n    Factorb.set_index('secID', inplace=True)\n \n    buy_list = Factor.index\n    sell_list = Factorb.index\n    prices = account.referencePrice \n    last10day=cal.advanceDate(account.previous_date,'-10B')\n    yesterdaya=cal.advanceDate(account.previous_date,'-1B')\n    hist=DataAPI.MktIdxdGet(indexID=u\"000001.ZICN\",beginDate=last10day,endDate=yesterdaya,field=u\"PrecloseIndex\",pandas=\"1\")['preCloseIndex']\n    \n    if stoploss(hist,-0.1):\n        for stock in account.valid_secpos:\n            order_to(stock,0)\n    if yesterday in date_list:     \n        for stock in account.universe:\n            for stock in sell_list:\n                if stock in account.valid_secpos :\n                    order_to(stock, 0)\n        for stock in account.universe:\n            for stock in buy_list:\n                if stock not in account.valid_secpos:\n                    order(stock, int(account.cash* Factor.loc[stock]['wts'] / prices[stock] /100)*100) \n\nbt,acct=quartz.backtest(start=start,\n            end=end,\n            benchmark=benchmark,\n            universe=universe,\n            capital_base=capital_base,\n            initialize=initialize,\n            handle_data=handle_data,\n            refresh_rate=refresh_rate)\n\nperf=quartz.perf_parse(bt,acct)\n    \nout_keys=['annualized_return','volatility','information_ratio',\n    'sharpe','max_drawdown','alpha','beta']\n\nfor k in out_keys:\n    print'%s:%s'%(k,perf[k])\n\nperf['cumulative_returns'].plot()\nperf['benchmark_cumulative_returns'].plot()\npylab.legend(['current_strategy','000001.ZICN'])          \n\nprint(\"ABC\")",
